{
  "name": "finrep",
  "version": "1.0.0",
  "author": "ringill",
  "license": "MIT",
  "private": true,
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon --inspect dist/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn run client\"",
    "start": "node src/server.js",
    "build": "yarn run build-ts && yarn run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Client\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\" \"yarn run client\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit",
    "debug": "yarn run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Client\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\" \"yarn run client\"",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "jest --coverage --watchAll",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "comm": "ts-node --project tsconfig.json src/parser/index.ts"
  },
  "dependencies": {
    "@types/errorhandler": "^0.0.32",
    "body-parser": "^1.19.0",
    "commander": "^4.1.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "pdfreader": "^1.0.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.1.8",
    "@types/source-map-support": "^0.5.1",
    "concurrently": "^5.0.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "serve": "^11.3.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
